  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm run test-dependencies"
    ]
  },
  "nyc": {
    "all": true,
    "cache": true,
    "check-coverage": true,
    "per-file": false,
    "lines": 55,
    "statements": 55,
    "functions": 55,
    "branches": 55,
    "include": [
      "src"
    ]
  }
