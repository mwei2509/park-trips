  "description": "",
  "main": "src/app.js",
  "scripts": {
    "start": "node bin/server.js",
    "smoke": "./node_modules/mocha/bin/_mocha __smoke__ --recursive --timeout=30000 --exit",
    "debug": "nodemon --inspect=0.0.0.0:5858 bin/server.js",
    "rollback": "node_modules/.bin/sequelize db:migrate:undo:all",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "lint": "node_modules/.bin/eslint .",
    "lint-fix": "node_modules/.bin/eslint --fix .",
    "lint-diff": "node_modules/.bin/eslint --cache .",
    "test": "NODE_ENV=test npm run rollback && NODE_ENV=test npm run migrate && NODE_ENV=test PORT=14321 ./node_modules/mocha/bin/_mocha __tests__ --exclude **/dependencies.test.js --recursive --timeout=20000 --exit",
    "test-coverage": "rm -rf coverage && rm -f coverage.lcov && ./node_modules/nyc/bin/nyc.js --reporter=lcovonly npm test && ./node_modules/nyc/bin/nyc.js report && ./node_modules/nyc/bin/nyc.js report --reporter=text-lcov > coverage.lcov",
    "test-dependencies": "node_modules/mocha/bin/_mocha __tests__/dependencies.test.js --timeout=20000"
  },
  "author": "mwei2509",
  "license": "ISC",
